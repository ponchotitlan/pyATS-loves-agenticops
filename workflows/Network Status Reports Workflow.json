    {
      "parameters": {
        "jsCode": "const items = $input.all();\n\n// Extract the ticketing agent prompt (same for all)\nconst ticketingAgentPrompt = items.find(item => item.json.ticketing_agent_prompt)?.json.ticketing_agent_prompt;\n\n// Get outputs and extract their IDs\nconst outputs = items.filter(item => item.json.output).map(item => {\n  // Extract ID from \"**Report ID: XYZ**\" pattern\n  const idMatch = item.json.output.match(/\\*\\*Report ID:\\s*([^\\*\\n]+)\\*\\*/);\n  const id = idMatch ? idMatch[1].trim() : null;\n  return {\n    id: id,\n    output: item.json.output\n  };\n});\n\n// Get report URLs and their tags (which contain the ID)\nconst reportUrls = items.filter(item => item.json.report_url).map(item => {\n  return {\n    id: item.json.report_tag, // \"R-01\", \"R-02\", etc.\n    url: item.json.report_url,\n    tag: item.json.report_tag\n  };\n});\n\n// Merge by ID\nconst consolidated = outputs.map(outputItem => {\n  // Find matching report URL by ID\n  const matchingReport = reportUrls.find(report => report.id === outputItem.id);\n  \n  return {\n    json: {\n      id: outputItem.id,\n      ticketing_agent_prompt: ticketingAgentPrompt,\n      output: outputItem.output,\n      report_url: matchingReport?.url || '',\n      report_tag: matchingReport?.tag || ''\n    }\n  };\n});\n\nreturn consolidated;"
      },
